<div class="weatherWidget">
	{{#each partials.weather.locations}}
		<div class="location">
			<h3>{{name}}</h3>
			<a href="{{forecastUrl}}">
				<img src="{{iconUrl}}" alt="{{weather}}">
				{{weather}}, {{temp}}
			</a>
		</div>
	{{/each}}
	<small>
		source: <a href="http://wunderground.com">Weather Underground</a>
	</small>
</div>